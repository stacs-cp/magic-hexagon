language ESSENCE' 1.0

given order: int
given shift: int
letting ix be order - 1
letting min_val be shift
letting max_val be shift - 1 + 3 * order ** 2 - 3 * order + 1
letting total_sum be sum([i | i : int(min_val..max_val)])
letting let1 be -ix
letting let2 be -ix
letting let3 be -ix
find hexagon: matrix indexed by [int(let1..ix), int(let2..ix), int(let3..ix)] of int(0, min_val..max_val)
find sumsto: int(0..total_sum)
find A: int(0..max_val)
find B: int(0..max_val)
find C: int(0..max_val)
find D: int(0..max_val)
find E: int(0..max_val)
find F: int(0..max_val)
branching on [hexagon, sumsto, A, B, C, D, E, F]
such that
    min_val > 0 ->
    and([hexagon[q1, j, k] != 0 | q1 : int(-ix..ix), j : int(-ix..ix), k : int(-ix..ix), q1 + j + k = 0]),
    and([hexagon[q2, j, k] = 0 | q2 : int(-ix..ix), j : int(-ix..ix), k : int(-ix..ix), q2 + j + k != 0]),
    allDiff([hexagon[q3, j, k] | q3 : int(-ix..ix), j : int(-ix..ix), k : int(-ix..ix), q3 + j + k = 0]),
    and([sumsto = sum([hexagon[q4, j, k] | j : int(-ix..ix), k : int(-ix..ix)]) | q4 : int(-ix..ix)]),
    and([sumsto = sum([hexagon[q5, j, k] | q5 : int(-ix..ix), k : int(-ix..ix)]) | j : int(-ix..ix)]),
    and([sumsto = sum([hexagon[q6, j, k] | q6 : int(-ix..ix), j : int(-ix..ix)]) | k : int(-ix..ix)]),
    A = hexagon[0, ix, -ix],
    B = hexagon[ix, 0, -ix],
    C = hexagon[ix, -ix, 0],
    D = hexagon[-ix, 0, ix],
    E = hexagon[0, -ix, ix],
    F = hexagon[-ix, ix, 0],
    [A, B, C, D, E, F; int(1..6)] <=lex [B, C, D, E, F, A; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [C, D, E, F, A, B; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [D, E, F, A, B, C; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [E, F, A, B, C, D; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [F, A, B, C, D, E; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [A, F, E, D, C, B; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [B, A, F, E, D, C; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [C, B, A, F, E, D; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [D, C, B, A, F, E; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [E, D, C, B, A, F; int(1..6)],
    [A, B, C, D, E, F; int(1..6)] <=lex [F, E, D, C, B, A; int(1..6)]

